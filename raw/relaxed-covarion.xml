	<!-- The unique patterns from 1 to end                                       -->
	<!-- npatterns=211                                                           -->
	<patterns id="patterns">
		<alignment idref="alignment"/>
	</patterns>



    <!-- This is a simple constant population size coalescent model              -->
    <!-- that is used to generate an initial tree for the chain.                 -->
    <constantSize id="initialDemo" units="years">
    	<populationSize>
    		<parameter id="initialDemo.popSize" value="100.0"/>
    	</populationSize>
    </constantSize>
    <!-- Generate a random starting tree under the coalescent process            -->
    <!-- Has calibration                                                         -->
    <coalescentTree id="startingTree">
    	<constrainedTaxa>
    		<taxa idref="taxa"/>
    		<tmrca monophyletic="true">
    			<taxa idref="MoreCojubim"/>
    			<uniformDistributionModel> 
    				<lower>200.0</lower>
    				<upper>300.0</upper>
    			</uniformDistributionModel>
    		</tmrca>
    		<tmrca monophyletic="true">
    			<taxa idref="TapakuraKitemoka"/>
    			<uniformDistributionModel> 
    				<lower>500.0</lower>
    				<upper>1000.0</upper>
    			</uniformDistributionModel>
    		</tmrca>
    		<tmrca monophyletic="true">
    			<taxa idref="ToraClade"/>
    			<uniformDistributionModel> 
    				<lower>400.0</lower>
    				<upper>1000.0</upper>
    			</uniformDistributionModel>
    		</tmrca>
    	</constrainedTaxa>
    	<constantSize idref="initialDemo"/>
    </coalescentTree>

	<!-- Generate a tree model                                                   -->
	<treeModel id="treeModel">
		<coalescentTree idref="startingTree"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>

	<!-- Taxon Sets                                                              -->
	<tmrcaStatistic id="tmrca(MoreCojubim)" includeStem="false">
		<mrca>
			<taxa idref="MoreCojubim"/>
		</mrca>
		<treeModel idref="treeModel"/>
	</tmrcaStatistic>
	<tmrcaStatistic id="tmrca(TapakuraKitemoka)" includeStem="false">
		<mrca>
			<taxa idref="TapakuraKitemoka"/>
		</mrca>
		<treeModel idref="treeModel"/>
	</tmrcaStatistic>
	<tmrcaStatistic id="tmrca(ToraClade)" includeStem="true">
		<mrca>
			<taxa idref="ToraClade"/>
		</mrca>
		<treeModel idref="treeModel"/>
	</tmrcaStatistic>

	<!-- Generate a generalizedSkyLineLikelihood for Bayesian Skyline            -->
	<generalizedSkyLineLikelihood id="skyline" linear="false">
		<populationSizes>
			<parameter id="skyline.popSize" dimension="3" value="10000.0" lower="0.0"/>
		</populationSizes>
		<groupSizes>
			<parameter id="skyline.groupSize" dimension="3"/>
		</groupSizes>
		<populationTree>
			<treeModel idref="treeModel"/>
		</populationTree>
	</generalizedSkyLineLikelihood>
	<exponentialMarkovLikelihood id="eml1" jeffreys="true">
		<chainParameter>
			<parameter idref="skyline.popSize"/>
		</chainParameter>
	</exponentialMarkovLikelihood>

    <!-- The uncorrelated relaxed clock (Drummond, Ho, Phillips & Rambaut (2006) PLoS Biology 4, e88 )-->
    <discretizedBranchRates id="branchRates">
    	<treeModel idref="treeModel"/>
    	<distribution>
    		<logNormalDistributionModel meanInRealSpace="true">
    			<mean>
    				<parameter id="ucld.mean" value="3.5e-5" lower="0.0" upper="1.0" />
    			</mean>
    			<stdev>
    				<parameter id="ucld.stdev" value="0.3333333333333333" lower="0.0"/>
    			</stdev>
    		</logNormalDistributionModel>
    	</distribution>
    	<rateCategories>
    		<parameter id="branchRates.categories"/>
    	</rateCategories>
    </discretizedBranchRates>
    <rateStatistic id="meanRate" name="meanRate" mode="mean" internal="true" external="true">
    	<treeModel idref="treeModel"/>
    	<discretizedBranchRates idref="branchRates"/>
    </rateStatistic>
    <rateStatistic id="coefficientOfVariation" name="coefficientOfVariation" mode="coefficientOfVariation" internal="true" external="true">
    	<treeModel idref="treeModel"/>
    	<discretizedBranchRates idref="branchRates"/>
    </rateStatistic>
    <rateCovarianceStatistic id="covariance" name="covariance">
    	<treeModel idref="treeModel"/>
    	<discretizedBranchRates idref="branchRates"/>
    </rateCovarianceStatistic>


	<!-- The Binary covarion model                                               -->
	<binaryCovarionModel id="bcov">
		<frequencies>
			<parameter id="frequencies" value="0.5637499994883304 0.4362500005116697"/>
		</frequencies>
		<hiddenFrequencies>
			<parameter id="hfrequencies" dimension="2" value="0.5" lower="0.0" upper="1.0"/>
		</hiddenFrequencies>
		<alpha>
			<parameter id="bcov.alpha" value="0.5" lower="0.0" upper="1.0"/>
		</alpha>
		<switchingRate>
			<parameter id="bcov.s" value="0.5" lower="0.0"/>
		</switchingRate>
	</binaryCovarionModel>

	<!-- site model                                                              -->
	<siteModel id="siteModel">
		<substitutionModel>
			<binaryCovarionModel idref="bcov"/>
		</substitutionModel>
	</siteModel>

	<!-- Likelihood for tree given sequence data                                 -->
	<treeLikelihood id="treeLikelihood" useAmbiguities="true">
		<patterns idref="patterns"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="siteModel"/>
        <discretizedBranchRates idref="branchRates"/>
	</treeLikelihood>

	<!-- Define operators                                                        -->
	<operators id="operators">
	    <scaleOperator scaleFactor="0.75" weight="0.1">
			<parameter idref="bcov.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="0.1">
			<parameter idref="bcov.s"/>
		</scaleOperator>
		<deltaExchange delta="0.01" weight="0.1">
			<parameter idref="hfrequencies"/>
		</deltaExchange>
        <scaleOperator scaleFactor="0.75" weight="3">
        	<parameter idref="ucld.mean"/>
        </scaleOperator>
        <scaleOperator scaleFactor="0.75" weight="3">
        	<parameter idref="ucld.stdev"/>
        </scaleOperator>
		<subtreeSlide size="5" gaussian="true" weight="15">
			<treeModel idref="treeModel"/>
		</subtreeSlide>
		<narrowExchange weight="15">
			<treeModel idref="treeModel"/>
		</narrowExchange>
		<wideExchange weight="3">
			<treeModel idref="treeModel"/>
		</wideExchange>
		<wilsonBalding weight="3">
			<treeModel idref="treeModel"/>
		</wilsonBalding>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="treeModel.rootHeight"/>
		</scaleOperator>
		<uniformOperator weight="30">
			<parameter idref="treeModel.internalNodeHeights"/>
		</uniformOperator>
		<scaleOperator scaleFactor="0.75" weight="15">
			<parameter idref="skyline.popSize"/>
		</scaleOperator>
		<deltaExchange delta="1" integer="true" weight="6" autoOptimize="false">
			<parameter idref="skyline.groupSize"/>
		</deltaExchange>
        <upDownOperator scaleFactor="0.75" weight="3">
        	<up>
        		<parameter idref="ucld.mean"/>
        	</up>
        	<down>
        		<parameter idref="treeModel.allInternalNodeHeights"/>
        	</down>
        </upDownOperator>
        <swapOperator size="1" weight="10" autoOptimize="false">
        	<parameter idref="branchRates.categories"/>
        </swapOperator>
        <uniformIntegerOperator weight="10">
        	<parameter idref="branchRates.categories"/>
        </uniformIntegerOperator>
	    <randomWalkIntegerOperator weight="10" windowSize="1">
	      <parameter idref="branchRates.categories"/>
	    </randomWalkIntegerOperator>
	</operators>

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="20000000"  autoOptimize="true" operatorAnalysis="relaxed-covarion.ops">
		<posterior id="posterior">
			<prior id="prior">
                <logNormalPrior mean="250.0" stdev="0.6" offset="236.0" meanInRealSpace="true">
                	<statistic idref="tmrca(TapakuraKitemoka)"/>
                </logNormalPrior>

                <logNormalPrior mean="201.0" stdev="0.6" offset="179.0" meanInRealSpace="true">
                	<statistic idref="tmrca(MoreCojubim)"/>
                </logNormalPrior>

                <logNormalPrior mean="300.0" stdev="0.4" offset="170.0" meanInRealSpace="true">
                	<statistic idref="tmrca(ToraClade)"/>
                </logNormalPrior>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="bcov.alpha"/>
				</uniformPrior>
				<gammaPrior shape="0.05" scale="10.0" offset="0.0">
					<parameter idref="bcov.s"/>
				</gammaPrior>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="hfrequencies"/>
				</uniformPrior>
                <exponentialPrior mean="0.3333333333333333" offset="0.0">
            		<parameter idref="ucld.stdev"/>
            	</exponentialPrior>
            	<uniformPrior lower="0.0" upper="1.0">
            		<parameter idref="ucld.mean"/>
            	</uniformPrior>
				<uniformPrior lower="0.0" upper="10000">
					<parameter idref="skyline.popSize"/>
				</uniformPrior>
				<generalizedSkyLineLikelihood idref="skyline"/>
				<exponentialMarkovLikelihood idref="eml1"/>
			</prior>
			<likelihood id="likelihood">
				<treeLikelihood idref="treeLikelihood"/>
			</likelihood>
		</posterior>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="1000">
			<column label="Posterior" dp="4" width="12">
				<posterior idref="posterior"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="rootHeight" sf="6" width="12">
				<parameter idref="treeModel.rootHeight"/>
			</column>
            <column label="ucld.mean" sf="6" width="12">
        		<parameter idref="ucld.mean"/>
        	</column>
		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="1000" fileName="relaxed-covarion.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<tmrcaStatistic idref="tmrca(MoreCojubim)"/>
			<tmrcaStatistic idref="tmrca(TapakuraKitemoka)"/>
			<tmrcaStatistic idref="tmrca(ToraClade)"/>
			<parameter idref="bcov.alpha"/>
			<parameter idref="bcov.s"/>
			<parameter idref="frequencies"/>
			<parameter idref="hfrequencies"/>
			<parameter idref="skyline.popSize"/>
			<parameter idref="skyline.groupSize"/>
            <parameter idref="ucld.mean"/>
        	<parameter idref="ucld.stdev"/>
        	<rateStatistic idref="meanRate"/>
        	<rateStatistic idref="coefficientOfVariation"/>
        	<rateCovarianceStatistic idref="covariance"/>
			<treeLikelihood idref="treeLikelihood"/>
			<generalizedSkyLineLikelihood idref="skyline"/>
		</log>

		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="1000" nexusFormat="true" fileName="relaxed-covarion.time.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<discretizedBranchRates idref="branchRates"/>
			<posterior idref="posterior"/>
		</logTree>
		<logTree id="substTreeFileLog" logEvery="1000" nexusFormat="true" fileName="relaxed-covarion.subst.trees" branchLengths="substitutions">
			<treeModel idref="treeModel"/>
			<discretizedBranchRates idref="branchRates"/>
		</logTree>
	</mcmc>
	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
</beast>
